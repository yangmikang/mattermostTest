.menu-2depth-box{
  position:relative;
  display: flex;
  /* column-count: 5; */
  /* column-span: all; */
  /* column-gap: 0; */
  /* flex-basis: 172px; */
  /* flex-grow: 1; */
  width: 814px;
  height: 100%;
}
.menu-2depth {
  position:relative;
  display: grid;
  grid-template-columns: repeat(5, 1fr);
  grid-template-rows: repeat(1, minmax(48px, auto));
  grid-auto-flow: dense;
  colum-gap: 1px;
  padding: 0 1px 0 0;
  z-index: 10;
  background-color: #ffffff;
  display: -ms-grid;
  -ms-grid-columns: repeat(5, 1fr);
  -ms-grid-rows: repeat(5, minmax(48px, auto));
}

.menu-2depth > div {
	border-right:1px solid #EDEDED;
}
.menu-2depth > .depth-order-1{
    grid-column: 1;
    -ms-grid-column: 1;
    /* border-right: 1px solid #EBEBEB; */
}
.menu-2depth > .depth-order-2 {
    grid-column: 2;
    grid-auto-flow: dense;
    -ms-grid-column: 2;
}
.menu-2depth > .depth-order-3 {
    grid-column: 3;
    -ms-grid-column: 3;
}
.menu-2depth > .depth-order-4 {
    grid-column: 4;
    -ms-grid-column: 4;
}
.menu-2depth > .depth-order-5 {
    grid-column: 5;
    -ms-grid-column: 5;
}

/* 나머지 li 스타일 */
.menu-2depth > div > li{
  padding: 14px 16px;
  box-sizing: border-box;
  display: flex;
  flex-basis: 172px;
  flex-shrink: 1;
  width: 100%;
