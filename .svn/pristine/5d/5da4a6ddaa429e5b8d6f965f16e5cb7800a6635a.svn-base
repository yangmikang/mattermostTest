  display:flex;
  align-items:center;
  justify-content: space-between;
  /* margin:0 var(--krds-spacer-5); */
  border-bottom: 1px solid #ccc;
}

.menu-2depth > div > li > a::after, .menu-3depth > li > a::after{
  content:"";
  display:inline-block;
  width:2rem;
  height:2rem;
  background-image:url(../../img/component/common/ico_arr_20_right.svg);
  z-index: 1;
