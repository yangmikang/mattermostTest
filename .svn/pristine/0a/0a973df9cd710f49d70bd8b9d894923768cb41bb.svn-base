 function registerEvent() {
	  const title = document.getElementById("consult_name").value;
	  const start = document.getElementById("modalStart").value;
	  const end = document.getElementById("modalEnd").value;
	  const citySelect = document.getElementById("citySelect");
	  const cityValue = citySelect.value;
	  const cityText = citySelect.options[citySelect.selectedIndex].text;
	  const description =  document.getElementById("description").value;
	  
	  console.log()
	  if (!title || !start || !end) {
	    alert("모든 항목을 입력하세요.");
	    return;
	  }
	console.log("cityVal", cityValue)
	  const eventData = {
	    eventTitle: title,
	    startDatetime: start + 'T00:00:00',
	    endDatetime: end + 'T23:59:59',
	    eventDescription: description,
	    organizer: cityValue,
	    location: '', // 또는 cityText, 원하시는 대로 사용
	    contact: '',
	    uploadThumbnail: '',
	    externalLink: '',
	    eventStatus: 'UPCOMING'
	  };

	  axios.post('/distep/calendar/register.do', eventData)
	    .then(response => {
	      const saved = response.data;
	      calendar.addEvent({
	        id: saved.id,
	        title: saved.eventTitle,
	        start: saved.startDatetime,
	        end: saved.endDatetime,
	        allDay: true
	      });
	      closeModal();
	      alert('일정이 등록되었습니다.');
	    })
	    .catch(error => {
	      console.error('등록 오류:', error);
	      alert('일정 등록 실패');
	    });
	}

		
		// 일정 목록을 가져와서 리스트에 출력
		function loadCalendarList(currentPage = 1) {
			let data = { currentPage: currentPage };
			axios.post('/distep/calendar/listAjax.do',data)
			  .then(response => {
				const data= response.data.list || response.data;// list가 없으면 그냥 response.data
			    const list = data.content
			    const container = document.querySelector(".search-list");
			    container.innerHTML = '';

			    console.log("리스트 출력", list);
			    console.log("리스트 출력", response);

			    if (!Array.isArray(list) || list.length === 0) {
			      container.innerHTML = '<li class="li"><p>등록된 일정이 없습니다.</p></li>';
			      return;
			    }

			    list.forEach(event => {
			      const startDate = new Date(event.startDatetime).toLocaleDateString();
			      const endDate = new Date(event.endDatetime).toLocaleDateString();

			      const li = document.createElement("li");
			      
			      li.className = "li";
			      li.innerHTML = `
			        <div class="in">
			          <div class="card-top">
			            <span class="krds-badge bg3">\${event.eventStatus}</span>
			          </div>
			          <div class="card-body">
			            <div class="c-text">
			              <p class="c-tit"><span class="span">\${event.eventTitle}</span></p>
			              <p class="c-txt">\${event.eventDescription || ''}</p>
			              <p class="c-date">
			                <strong class="key">기간</strong>
			                <span class="value">\${startDate} ~ \${endDate}</span>
			              </p>
			            </div>
			          </div>
			        </div>
			      `;
			      
			      
			      
			      container.appendChild(li);
			    });

		      document.querySelector("#divPagingArea").innerHTML =data.pagingArea;
		      bindPagingEvents(); 
			  })
			  .catch(error => {
			    console.error('일정 목록 불러오기 실패:', error);
			  });

		}
		function bindPagingEvents() {
			  document.querySelectorAll(".clsPagingArea").forEach(function (el) {
			    el.addEventListener("click", function (e) {
			      e.preventDefault();
			      const currentPage = parseInt(this.dataset.currentPage);
			      console.log("page",currentPage)
			      loadCalendarList(currentPage);
			    });
			  });
			}
			
		
		
		
		

	</script>
