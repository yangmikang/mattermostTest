<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<script>
function alert_close(){
	$('#alertView').hide();
}
</script>


<div class="modal_wrap act" id="alertView" style="display: none; z-index:9998;">        
    <div class="alert">
        <button class="btn_close btn_close_bk" onclick="alert_close();">닫기</button>
        <div class="alert_inner">
            <div>
                <h6>알림</h6>
                <p id="alertMsg"></p>   
                <div class="btn_wrap">
                <button class="btn" onclick="alert_close();">확인</button>        
                </div>
            </div>
        </div>
    </div>
</div>