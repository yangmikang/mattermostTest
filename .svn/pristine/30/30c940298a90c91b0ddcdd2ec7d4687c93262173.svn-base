  align-items: flex-start;
  justify-content: center;
  flex-direction: row;
  gap: var(--krds-spacer-10);
  height: 100%;
  background-position: center right;
  background-repeat: no-repeat;
}
.main-vban-wrap .vb-card .main-d-text {
