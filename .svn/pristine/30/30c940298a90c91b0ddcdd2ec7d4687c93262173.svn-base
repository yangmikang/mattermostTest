.network-align{padding:var(--krds-spacer-10) var(--krds-spacer-10);border: 1px solid #ccc;color: #222;gap: 3rem;}
#container > .inner .gini-cont-wrap .gini-cont-box.network-align > .row{height:stretch;}

.ico-bullet{display:flex; align-items:center;}
.ico-bullet::before{content:""; display:inline-block; width:.2rem; height:1.6rem; background-color:var(--krds-primary-70); margin-right:var(--krds-spacer-2);}
.bg-imgType .ico-bullet::before{background-color:var(--krds-gray-50);}

.ico-bullet2{display:flex; align-items:center;}
.ico-bullet2::before{content:""; display:inline-block; width:1.6rem; height:1.6rem; background-image:url(../../img/component/common/ico-cube.svg); background-repeat:no-repeat; background-size:contain; background-position:contain; margin-right:var(--krds-spacer-2);}
.bg-imgType.type01 .ico-bullet2::before{filter: invert(100%) sepia(8%) saturate(26%) hue-rotate(179deg) brightness(110%) contrast(100%);}

.bg-color-box{background:var(--krds-primary-5); border-radius:var(--krds-rd-12);}

#container > .inner .gini-cont-wrap .bg-color-box .gini-cont-box > .row{background:transparent;}
#container > .inner .gini-cont-wrap .gini-cont-box .bg-color-box .row{display: flex; gap:var(--krds-spacer-10);}

.cont-map{position:relative; }
.map-title-box{position:absolute; left:5%; top:5%; color:var(--krds-white);}
.map-title-box h3{font-size:var(--krds-fz-heading-md);}
.map-title-box .ico-hint{display:flex; align-items:center; opacity:.8;}
.map-title-box .ico-hint::before{content:""; display:inline-block; width:2rem; height:2rem; border-radius:50%; filter: invert(100%) sepia(8%) saturate(26%) hue-rotate(179deg) brightness(110%) contrast(100%); background-image:url(../../img/component/common/ico-info.svg); background-size:contain; background-position:center; background-repeat:no-repeat; margin-right:var(--krds-spacer-1);}

.map-area{border-radius:4rem; overflow:hidden;}
.map-btn-box a{position:absolute; display:inline-block;}
.map-btn-box a::before{content:"";display:inline-block; width:6rem; height:4rem; background-image:url(../../img/component/common/ico_location.svg); background-size:50%; background-position:center; background-repeat:no-repeat; filter: invert(100%) sepia(100%) saturate(2%) hue-rotate(81deg) brightness(103%) contrast(101%);}
.btn-daejeon{right:22%; top:46%;}
.btn-dortmund{left:49%; top:39%;}
.btn-malaga{left:47%; top:46%;}
.btn-montgomery{left:33%; top:47%;}
.btn-seattle{left:26%; top:44%;}
.btn-quebec{left:35%; top:40%;}
.btn-hinschu{right:24%; top:51%;}
.map-btn-box .city-name{color:var(--krds-white); display: block; text-shadow: 1px 1px 3px #000; max-width:15rem; margin-left:-3rem; text-align: center;}
.map-btn-box a{transition:all .3s ease;}
.map-btn-box a:hover{transform:scale(1.1); z-index:1;}
.city-flag{display:inline-block; width:4rem; height:4rem; margin-left:.8rem;}
.city-flag::before{content:""; display:inline-block; width:100%; height:100%; background-position:center; background-size:contain; background-repeat:no-repeat;}
.city-flag.ico-kor::before{background-image:url(../../img/pattern/common/ico_flag_korean.png);}
.city-flag.ico-dortmund::before{background-image:url(../../img/component/common/ico_germany.svg);}
.city-flag.ico-malaga::before{background-image:url(../../img/component/common/ico_spain.svg);}
.city-flag.ico-montgomery::before{background-image:url(../../img/component/common/ico_usa.svg);}
.city-flag.ico-seattle::before{background-image:url(../../img/component/common/ico_usa.svg);}
.city-flag.ico-quebec::before{background-image:url(../../img/component/common/ico_canada.svg);}
.city-flag.ico-hisnchu::before{background-image:url(../../img/component/common/ico_taiwan.svg);}

.cont-map .ico::before{content: "";
    display: inline-block;
    width: 1.5rem;
    height: 1.5rem;
    background-repeat: no-repeat;
    background-size: contain;
    background-position: center;
    margin-right: var(--krds-spacer-3);}

.cont-map .info-list > li{padding:0;}

.cont-map .info-list{display: flex; gap:var(--krds-spacer-10); align-items:center;}
.cont-map .info-list li{margin-top:0;}


.cont-about{position:relative; margin-bottom:var(--krds-spacer-20);}
.cont-about .img-box{border-radius:var(--krds-rd-12); height:40rem; max-height:initial; width:80%;}
.story-box{
  position:absolute;
  right:0;
  bottom:-10%;
  height:30rem;
  width:65%;
  max-width:70rem;
  background:rgba(15, 121, 149, .6);
  z-index:1;
  border-radius:var(--krds-rd-12);
  color:var(--krds-white);
  padding:var(--krds-spacer-10);
  display:flex;
  flex-direction: column;
  gap:var(--krds-spacer-5);
}

.mission-box{
  margin:var(--krds-spacer-10) 0;
}

.mission-box > ul{
  display:flex;
  flex-direction: column;
  align-items:center;
}

.mission-box > ul > li{
  display:inline-block;
  padding:var(--krds-spacer-6);
  /* border:2px solid var(--krds-primary-80); */

  min-width:40rem;
  border-radius:4rem;
  margin:var(--krds-spacer-6) 0;
  position:relative;
  background:var(--krds-primary-80);
  color:var(--krds-white);
  font-size:var(--krds-fz-body-md);
  flex-wrap: wrap;
}

.mission-box > ul > li:nth-child(2n){
  background:var(--krds-secondary-80);
}

.mission-box > ul > li.mission-field{
  border:0;
}

.mission-box > ul > li.mission-field ul{
  display:flex;
  flex-direction: row;
  align-items: center;
}

.mission-box > ul > li.mission-field ul li{
  border:1px solid var(--krds-white);
  border-radius:50%;
  width:18rem;
  height:18rem;
  margin:0 var(--krds-spacer-2);
  justify-content: center;
  background:rgba(255,255,255,.5);
  display:flex;
  text-align:center;
  flex-direction: column;
  font-weight:600;
}

.mission-box > ul > li::after{
  content:""; display:inline-block; width:2rem; height:2rem; background-image:url(../../img/component/common/ico-next-double.svg); background-repeat:no-repeat; background-position:center; background-size:contain;
  position:absolute;
  top:calc(100% + 1.2rem);
  left:50%;
  transform:translateX(-50%);
  opacity: .3;
}

.mission-box > ul > li:last-child::after{
  display:none;
}

.mission-field li::before{
  content:"";
  display:inline-block;
  width:100%;
  height:6rem;
  background-repeat:no-repeat;
  background-position:center;
  filter: invert(100%) sepia(0%) saturate(2%) hue-rotate(19deg) brightness(119%) contrast(100%);
}

.ico-science::before{background-image:url(../../img/component/common/ico-science.svg); background-size:25%;}
.ico-tech::before{background-image:url(../../img/component/common/ico-tech.svg);background-size:30%;}
.ico-innov::before{background-image:url(../../img/component/common/ico-inno.svg); background-size:25%;}

.assembly .cont-right{
  margin-top:var(--krds-spacer-10);
}

.sub-tit.ico-bullet2 ~ .desc{
  margin-left:2rem;
}

.forums .area-group{
  border-radius: var(--krds-rd-12);
  padding: var(--krds-spacer-10);
  background-color:rgba(0,0,0,.5);
  text-shadow: 1px 1px 2px rgba(0, 0, 0, .3);
}


.forums .bg-imgType{
  background-position:center;
  background-size:cover;
  background-repeat:no-repeat;
  background-image:url(../../img/component/common/img-forum.jpg);
}

#container > .inner .gini-cont-wrap.forums .gini-cont-box > .row{
  background-color:transparent;
}

.forums .bg-imgType .area-group .area-tit, .forums .bg-imgType .area-group .sub-tit, .forums .bg-imgType .area-group .desc{
  color:var(--krds-white);
}

 .forums .ico-bullet2::before{
  filter: invert(100%) sepia(0%) saturate(2%) hue-rotate(19deg) brightness(119%) contrast(100%);
 }

 .forums  .cont-left .area-group{
  height:100%;
 }

 .forums  .cont-right .area-group:last-child{
  margin-bottom:0;
 }

 .forums .bg-imgType{
  display:flex;
  gap:var(--krds-spacer-15);
  flex-direction: column;
 }



